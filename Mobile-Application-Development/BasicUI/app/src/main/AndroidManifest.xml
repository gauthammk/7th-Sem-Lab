<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basicui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TheGridView"></activity>
        <activity android:name=".TheListView" />
        <activity android:name=".TheDatePicker" />
        <activity android:name=".TheProgressBar" />
        <activity android:name=".TheAutocomplete" />
        <activity android:name=".TheSpinner" />
        <activity android:name=".TheCheckbox" />
        <activity android:name=".TheEditText" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>